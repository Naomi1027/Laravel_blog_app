{
    "regions": [
        "hnd1"
    ],
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.2"
        }
    },
    "routes": [
        {
            "src": "/build/assets/(.*)",
            "dest": "/public/build/assets/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/robots.txt",
            "dest": "/public/robots.txt"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "rewrites": [
        { "source": "/auth/google", "destination": "/index.php" },
        { "source": "/auth/google/callback", "destination": "/index.php" },
        { "source": "/(.*)", "destination": "/api/index.php" }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_NAME": "Laravel Vercel",
        "APP_DEBUG": "false",
        "APP_URL": "https://laravel-blog-app-psi.vercel.app",
        "APP_KEY": "base64:nQ6DylggHYrVlJ0cBqL2US+0SJ7YFlJKJmoZvrZB41E=",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "CACHE_DRIVER": "array",
        "CACHE_STORE": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "VIEW_COMPILED_PATH": "/tmp"
    }
}
